{
  "name": "mocha-with-docker",
  "version": "1.0.0",
  "description": "Example how to marry Mocha with Docker in order to make end-to-end even greater",
  "scripts": {
    "test": "mocha ./tests/**/*.test.js",
    "test:e2e": "mocha ./tests/end-to-end/*.test.js",
    "test:bar": "mocha ./tests/bar/*.test.js",
    "test:foo": "mocha ./tests/foo/*.test.js"
  },
  "author": "Marek Siemieniuk-Morawski",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.5",
    "mocha": "^8.1.3",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5"
  }
}
